//VADAGA ARUNA KUMARI  awdckkd  Assignment-1
#include <stdio.h>    
     
int main()    
{    

    int arr[50],n,temp,i,j;    
      

    printf("Enter the size of the array  :");
    scanf("%d",&n);
        
    printf("Enter the elements of  array: \n");    
    for (int i = 0; i < n; i++)      
        scanf("%d", &arr[i]);     
    
    for (i = 0; i < n; i++) 
    {     
        for ( j = i+1; j < n; j++) 
        {     
           if(arr[i] > arr[j]) 
           {    
               temp = arr[i];    
               arr[i] = arr[j];    
               arr[j] = temp;    
           }     
        }     
    }    
        
    printf("Elements of array in a sequential order: \n");    
    for (i = 0; i < n; i++)     
        printf("%d  ", arr[i]);    
    return 0;    
}     
